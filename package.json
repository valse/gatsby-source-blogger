{
  "name": "gatsby-source-blogger",
  "version": "1.2.1",
  "description": "Gatsby source plugin for building websites using Blogger as a data source",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin",
    "blogger",
    "headless-cms"
  ],
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "format": "prettier --single-quote --write src/gatsby-node.js",
    "prepare": "npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__"
  },
  "main": "index.js",
  "repository": "git@github.com:valse/gatsby-source-blogger.git",
  "author": "Marco Valsecchi <marco.valsecchi@gmail.com> (https://marcovalsecchi.it/)",
  "license": "MIT",
  "dependencies": {
    "googleapis": "^61.0.0",
    "rehype-parse": "^7.0.1",
    "rehype-remark": "^8.0.0",
    "remark-stringify": "^8.1.1",
    "unified": "^9.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "prettier": "^2.1.2"
  }
}